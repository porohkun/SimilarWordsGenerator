<Page x:Class="SimWordsGenApp.Pages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SimWordsGenApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="25" d:DesignWidth="300"
      Title="MenuPage">
    <Grid>
        <RadioButton Content="Profile" Click="Profile_Click"  Width="100" HorizontalAlignment="Left" Style="{StaticResource {x:Type ToggleButton}}"/>
        <RadioButton Content="Generator" Click="Generator_Click"  Width="100" Margin="100,0,0,0" HorizontalAlignment="Left" Style="{StaticResource {x:Type ToggleButton}}"/>
    </Grid>
</Page>
